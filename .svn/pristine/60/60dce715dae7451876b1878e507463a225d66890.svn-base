/*观看状态*/
function watchstate(state,id,cartoonId,sort,titile){
	$.ajax({
		url : "/Comic/qpp/comic/update/cartoonSet.do",
		type:"post",
		data:{
			"watchState": state,
			"id" : id,
			"cartoonId" : GetQueryString("id"),
			"sort" : sort,
			"titile" : titile
		},
		success:function(data){
			if(data.error == 200){
				mallwatch();
			}else{
				alert(data.msg)
			}
		}
	});
}
